																else
																	thisModule.action <- op3.guard.regexReplaceAll('\'', '"').regexReplaceAll(' = ',' == ').regexReplaceAll(' <> ',' != ').regexReplaceAll(' and ',' && ').regexReplaceAll(' or ',' || ');
